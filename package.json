{
  "name": "faasjs-workflow",
  "version": "1.0.0",
  "type": "module",
  "workspaces": ["packages/*"],
  "packageManager": "npm@11.0.0",
  "scripts": {
    "lint": "biome lint .",
    "format": "biome format --write .",
    "test": "vitest run",
    "ci": "vitest run --silent --coverage",
    "build": "npm run build --workspaces --if-present",
    "publish:devel": "node devel.cjs",
    "clean": "rm -rf packages/*/dist coverage"
  },
  "devDependencies": {
    "@types/node": "*",
    "@types/react": "*",
    "tsup": "*",
    "@faasjs/lint": "*",
    "vitest": "*",
    "@vitejs/plugin-react": "*",
    "happy-dom": "*",
    "vite-tsconfig-paths": "*"
  },
  "engines": {
    "npm": ">=11.0.0",
    "node": ">=22.0.0"
  }
}
