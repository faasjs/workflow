{
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "strict": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "strictNullChecks": false,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "target": "ESNext",
    "module": "ESNext",
    "baseUrl": ".",
    "paths": {
      "@faasjs/workflow-*": [
        "./packages/*"
      ],
      "@faasjs/*:": [
        "./node_modules/@faasjs/*/dist/index.mjs"
      ]
    }
  }
}
